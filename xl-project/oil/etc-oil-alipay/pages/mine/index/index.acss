    padding: 25rpx 40rpx;
    box-sizing: border-box;
}

.pop-up-hint-title {
    color: rgba(136, 136, 136, 1);
    font-size: 28rpx;
    text-align: center;
    font-family: PingFangSC-Regular;
}

.pop-up-operation-view {
    width: 100%;
    background-color: #FFFFFF;
    border-bottom: 10rpx solid rgba(205, 205, 209, 0.3);
    text-align: center;
    padding: 25rpx 40rpx;
    box-sizing: border-box;
}

.pop-up-operation {
    color: rgba(230, 67, 64, 1);
    font-size: 36rpx;
    text-align: center;
    font-family: PingFangSC-Regular;
}

.pop-up-cancel-view {
    width: 100%;
    background-color: #FFFFFF;
    text-align: center;
    padding: 25rpx 40rpx;
    box-sizing: border-box;
}

.pop-up-cancel {
    color: rgba(0, 0, 0, 1);
    font-size: 36rpx;
    text-align: center;
    font-family: PingFangSC-Regular;
}

