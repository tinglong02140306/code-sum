/* 清除浮动 */

.clear {
    clear: both;
    overflow: hidden;
    height: 0;
}

.fly-form {
    *zoom: 1;
}

.fly-form:before,
.fly-form:after {
    content: "";
    display: table;
    font: 0/0 a;
}

.fly-form:after {
    clear: both;
}

.fly-btn {
    padding: 0 2em;
    border: 1px solid #999999;
    border-radius: 3px;
    font-size: 14px;
    background-color: #fff;
    color: #999999;
    height: 28px;
    line-height: 24px;
    vertical-align: top;
}

.fly-btn:hover {
    outline: 0;
    color: #999999;
    border-color: gray;
    text-decoration: none;
}

.fly-btn.fly-btn-disabled,
.fly-btn[disabled] {
    pointer-events: none;
    color: #999999;
    background-color: #eeeeee;
    border-color: #999999;
    cursor: not-allowed;
    -webkit-box-shadow: none;
    box-shadow: none;
}

.fly-btn-primary {
    color: #fff;
    background-color: #0e90d2;
    border-color: #0c80ba;
}

.fly-btn-primary:hover {
    color: #fff;
    background-color: #0c80ba;
    border-color: #0b6fa2;
}

.fly-checkbox {
    line-height: 30px;
}

.fly-checkbox label {
    margin: 0 20px 0 0;
}

.fly-checkbox input {
    vertical-align: -2px;
}

.fly-combobox-arrow {
    float: right;
    width: 23px;
    height: 28px;
    border-left: 1px solid #e4e4e4;
    background-color: #f9f9f9;
    background-image: url(../img/icon/icon-combobox.png);
    background-repeat: no-repeat;
    background-position: center 0;
    text-align: center;
    cursor: pointer;
}

.fly-combobox-arrow:hover {
    background-color: #f2f2f2;
}

.fly-combobox-active .fly-combobox-arrow {
    background-position: center -28px;
}

.fly-combobox-multi.fly-combobox-active .fly-combobox-arrow {
    background-position: center -56px;
}

.fly-combobox-clear.fly-combobox-active .fly-combobox-arrow {
    background-position: center -56px;
}

.fly-combobox {
    position: relative;
    line-height: 28px;
    border: 1px solid #e4e4e4;
    background-color: #fff;
}

.fly-combobox em {
    display: block;
    margin: 0 3px;
    padding: 0 5px;
    margin-right: 27px;
}

.fly-combobox .text {
    margin: 0 0 0 -8px;
    padding: 6px 8px;
    width: 100%;
    height: 16px;
    line-height: 16px;
    background: none;
    border: none;
    vertical-align: top;
}

.fly-combobox input.disabled {
    background-color: #f3f3f3;
}

.fly-combobox-inner {
    position: absolute;
    display: none;
    margin: 0 0 0 -1px;
    width: 100%;
    border: 1px solid #e4e4e4;
    background-color: #fff;
    z-index: 999;
    max-height: 156px;
    overflow: auto;
}

.fly-combobox-inner li {
    padding: 5px 8px;
    font-size: 12px;
    line-height: 16px;
    cursor: pointer;
    color: #6d6d6d;
    text-align: left;
}

.fly-combobox-inner li:hover {
    background-color: #f4f6f8;
}

.fly-combobox-inner li.selected {
    background-color: #D9E8FB;
}

.fly-combobox-tree li:hover {
    background: none;
}

.fly-combobox-tree li.selected {
    background: none;
}

