/* pages/mine/test/test.acss */

@import "../../../templates/mine/etc-card-add/etc-card-add.acss";
.mine-info-container {
    width: 100%;
    height: auto;
    box-sizing: border-box;
    overflow-x: hidden;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.mine-info-container-content {
    width: 100%;
    height: auto;
    box-sizing: border-box;
    overflow-x: hidden;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: absolute;
    top: 0;
}

.mine-navigation-bar {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: rgba(255, 255, 255, 1);
    background-color: transparent;
    font-size: 30rpx;
    text-align: center;
    font-family: PingFangSC-Medium;
}

.mine-account-info-top-bg {
    width: 100%;
    /*width: 750rpx;*/
    height: 720rpx;
    position: relative;
}

.mine-account-info-box-green {
    top: 150rpx;
    width: 100%;
    /*height: 320rpx;*/
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    /* position: absolute; */
    box-sizing: border-box;
}


/*头像昵称*/

.mine-user-header-container {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    padding: 10rpx 0;
    width: 100%;
    box-sizing: border-box;
}

.head-image-box {
    margin-left: 36rpx;
    width: 86rpx;
    height: 86rpx;
    border-radius: 100%;
    overflow: hidden;
    box-shadow: 0rpx 12rpx 40rpx 0rpx rgba(0, 0, 0, 0.1);
}

.head-image {
    width: 86rpx;
    height: 86rpx;
}

.nick-name {
    flex: 1;
    margin-left: 24rpx;
    margin-right: 10rpx;
    color: #ffffff;
    font-size: 36rpx;
    text-align: left;
    font-family: PingFangSC-Semibold;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.name-wrap {
    flex: 1;
    margin-left: 24rpx;
    margin-right: 10rpx;
}

.name-wrap text {
    display: inline-block;
    width: 100%;
    color: #ffffff;
    font-size: 36rpx;
    text-align: left;
    font-family: PingFangSC-Semibold;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.un-login {
    border: none;
    background: transparent;
}

.mine-login-btn {
    width: 160rpx;
    height: 60rpx;
    border-radius: 32rpx;
    background-color: rgba(255, 255, 255, 0.15);
    color: rgba(255, 255, 255, 0.7);
    font-size: 28rpx;
    text-align: center;
    font-family: PingFang-SC-Medium;
    margin-right: 36rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    border: none;
}


/*ETC卡列表*/

.mine-etc-card-list-box {
    /*width: 750rpx;*/
    width: 100%;
    white-space: nowrap;
    height: 280rpx;
    display: flex;
    flex-direction: row;
    box-sizing: border-box;
}

.hor {
    display: flex;
    flex-direction: row;
    padding-right: 40rpx;
}

.etx-card-box {
    display: inline-block;
}

.mine-etc-card-item {
    /*display: inline-block;*/
    margin: 0rpx 10rpx 0rpx 20rpx;
    width: 622rpx;
    height: 260rpx;
    border-radius: 12rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: center;
    align-items: center;
}

.mine-etc-card-item-no {
    /*display: inline-block;*/
    display: flex;
    justify-content: center;
    align-items: center;
    margin: -16rpx 30rpx 0;
    width: 652rpx;
    /*height: 260rpx;*/
    border-radius: 12rpx;
    box-sizing: border-box;
}

.card {
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    height: 80vh;
    width: 80vw;
    margin: 5vh 10vw;
    font-size: 40px;
    background: #F8F2DC;
    border-radius: 4px;
}


/*ETC卡*/

.mine-etc-card-container {
