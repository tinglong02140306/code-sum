@import "../../../templates/mine/etc-card-add/etc-card-add.wxss";
.mine-info-container{
    width: 100%;
    height: auto;
    box-sizing: border-box;
    overflow-x: hidden;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.mine-info-container-content{
    width: 100%;
    height: auto;
    box-sizing: border-box;
    overflow-x: hidden;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: absolute;
    top: 0;
}
.mine-navigation-bar{
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: rgba(255, 255, 255, 1);
    background-color: transparent;
