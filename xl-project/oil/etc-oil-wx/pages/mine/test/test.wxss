    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    position: absolute;
    box-sizing: border-box;
}
/*头像昵称*/
.mine-user-header-container{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    padding: 10rpx 0;
    width: 100%;
    box-sizing: border-box;
}
.nick-name-phone-box{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
    box-sizing: border-box;
    flex: 1;
    margin-left: 24rpx;
    margin-right: 10rpx;
}
.head-image-box{
    margin-left: 36rpx;
    width: 86rpx;
    height: 86rpx;
    border-radius: 100%;
    overflow: hidden;
    box-shadow: 0rpx 12rpx 40rpx 0rpx rgba(0, 0, 0, 0.1);
}
.head-image{
    width: 86rpx;
    height: 86rpx;
}
.nick-name{
    flex: 1;
    margin-left: 24rpx;
    margin-right: 10rpx;
    color: #ffffff;
    font-size: 36rpx;
    text-align: left;
    font-family: PingFangSC-Semibold;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.mine-login-btn{
    width: 160rpx;
    height: 60rpx;
    border-radius: 32rpx;
    background-color: rgba(255, 255, 255, 0.15);
    color: rgba(255, 255, 255, 0.7);
    font-size: 28rpx;
    text-align: center;
    font-family: PingFang-SC-Medium;
    margin-right: 36rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}
/*ETC卡列表*/
.mine-etc-card-list-box{
    /*width: 750rpx;*/
