/* pages/stations/clean-details/clean-details.wxss */
.clean-details-container{
    width: 100%;
    display: flex;
    flex-direction: column;
}

.clean-details-image{
    width: 100%;
    height: 350rpx;
}

.clean-details-box{
    width: 100%;
    position: relative;
    left: 0;
    top: -40rpx;
    z-index: 2;
    padding: 0 32rpx;
    box-sizing: border-box;
}

.clean-details-info{
    width: 690rpx;
    padding: 24rpx;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(0, 0, 0, 0.05);
}

.clean-details-info-top{
    width: 100%;
    display: flex;
    flex-direction: row;
    box-sizing: border-box;
    overflow: hidden;
    align-items: center;
}

.clean-details-name{
    color: #333333;
    font-size: 42rpx;
    font-weight: bolder;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}

/*地址*/
.clean-details-address{
    width: 100%;
    height: 96rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    overflow: hidden;
}

.clean-details-address-text{
    flex: 1;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    color: #999999;
    font-size: 26rpx;
}

.clean-details-navigation{
    min-width: 100rpx;
    margin-left: 32rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.clean-details-distance{
    color: #333333;
    font-size: 24rpx;
}

.clean-details-navigation-logo{
    width: 52rpx;
    height: 52rpx;
    margin-left: 16rpx;
}

/**价格状态**/
.clean-item-box-bottom{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    box-sizing: border-box;
    align-items: flex-end;
    overflow: hidden;
    margin-top: -20rpx;
}

.clean-item-box-bottom-left{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: flex-end;
}

/**价格**/
.clean-item-price-box{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: flex-end;
}

.clean-item-price-marker{
    font-size: 24rpx;
    color: rgba(0, 0, 0, .85);
}

.clean-item-price-money{
    font-weight: bold;
    font-size: 42rpx;
    color: rgba(0, 0, 0, .85);
    position: relative;
    bottom: -6rpx;
}

/**状态**/
.clean-item-status-box{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-left: 20rpx;
}

.clean-item-status-image{
    width: 29rpx;
    height: 29rpx;
}
.clean-item-box-bottom-right{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}
.clean-item-status-image-refresh{
    margin: 0 32rpx 0 14rpx;
    width: 28rpx;
    height: 28rpx;
}

.clean-item-status-text{
    font-size: 24rpx;
    color: rgba(0, 0, 0, .85);
    margin-left: 4rpx;
}

.clean-item-box-info-btn{
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    overflow: hidden;
    margin-right: 32rpx;
}
.clean-item-clean-time{
    /*height: 30rpx;*/
    color: rgba(51, 51, 51, 1);
    font-size: 24rpx;
    text-align: right;
    font-family: PingFang-SC-Medium;
}
/*停车*/
.clean-item-stop-box{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: flex-end;
    margin-top: 14rpx;
}

.clean-details-stop-bg{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 82rpx;
    height: 40rpx;
    border-radius: 2rpx;
    background-color: rgba(5, 127, 255, 0.05);
    border: 2rpx solid rgba(22, 136, 255, 0.25);
    color: rgba(5, 127, 255, 1);
    font-size: 24rpx;
    text-align: left;
    font-family: PingFangSC-Regular;
}

.clean-details-stop{
    height: 40rpx;
    color: rgba(5, 127, 255, 1);
    font-size: 26rpx;
    text-align: left;
    font-family: PingFang-SC-Medium;
    margin-left: 10rpx;
}

/*洗车模式*/
.clean-model-box{
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    align-items: center;
    padding: 32rpx;
}
.clean-model-box-top{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    box-sizing: border-box;
    align-items: center;
}
.clean-model-bottom-left{
    display: flex;
    flex-direction: column;
    justify-content: center;
    box-sizing: border-box;
    align-items: center;
    width: 330rpx;
    height: 186rpx;
    border-radius: 8rpx;
    background-color: rgba(255, 255, 255, 1);
    box-shadow: 0rpx 2rpx 12rpx 0rpx rgba(0, 161, 112, 0.45);
    border: 2rpx solid rgba(5, 186, 125, 1);
    margin-top: 24rpx;
}
.clean-model-bottom-right{
    display: flex;
    flex-direction: column;
    justify-content: center;
    box-sizing: border-box;
    align-items: center;
    width: 330rpx;
    height: 186rpx;
    border-radius: 8rpx;
    background-color: rgba(255, 255, 255, 1);
    margin-top: 24rpx;
}
.clean-model-bottom-icon{
    width: 160rpx;
    height: 98rpx;
    background-color: #F2F2F2;
}
.clean-model-bottom-text{
    color: rgba(167, 167, 167, 1);
    font-size: 24rpx;
    text-align: center;
    font-family: PingFangSC-Medium;
}
.clean-model-sub-title{
    display: flex;
    flex-direction: row;
    justify-content: center;
    box-sizing: border-box;
    align-items: center;
}
.clean-model-title{
    color: rgba(0, 0, 0, 0.85);
    font-size: 32rpx;
    font-weight: bolder;
}

.clean-model-title-hint{
    color: rgba(0, 0, 0, 0.45);
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    margin-right: 14rpx;
}

.clean-model-hint-img{
    width: 24rpx;
    height: 24rpx;
    background-color: rgba(0, 0, 0, 0.45);
}

.clean-share-activity-box{
    margin: 0 30rpx 30rpx 30rpx;
    color: rgba(0, 0, 0, 0.85);
    font-size: 32rpx;
    text-align: left;
    font-family: PingFangSC-Medium;
}

/*分享好友*/
.clean-share-container{
    /*margin-top: 10rpx;*/
    /*width: 100%;*/
    padding: 0 30rpx;
}
.clean-share-container-box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    /*position: relative;*/
