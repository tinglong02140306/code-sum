    font-family: PingFangSC-Medium;
}

.clean-share-box-left-btns{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    width: 102rpx;
    height: 36rpx;
    margin-top: 10rpx;
    border-radius: 20rpx;
    background-color: rgba(1, 161, 112, 1);
    color: #FFFFFF;
    font-size: 22rpx;
    text-align: left;
    font-family: PingFangSC-Regular;
}
.clean-share-box-left-icon{
    width: 16rpx;
    height: 8rpx;
    background-color: #e1e1e1;
}
.clean-share-box-right-icon{

    width: 225rpx;
    height: 141rpx;
    /*background-color: #05BA7D;*/
}

.clean-share-image-big{
    /*border-radius: 8rpx;*/
    width: 684rpx;
    height: 160rpx;
}
.clean-share-box-left{
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.clean-share-box-img{
    width: 300rpx;
    height: 188rpx;
    /*background-color: #F2F2F2;*/
}
.clean-share-service-btn{
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    opacity: 0;
}
.clean-share-box-left-text{
    width: 100%;
    color: #333333;
    font-size: 42rpx;
    text-align: left;
    font-weight: bolder;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}

.clean-share-box-left-sub{
    width: 100%;
    text-align: left;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    color: #999999;
    font-size: 26rpx;
}

/*一键洗车*/
.clean-details-btns{
    width: 100vw;
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    z-index: 2;
    background-color: #FFFFFF;
    padding-top: 24rpx;
    padding-bottom: 24rpx;
    box-shadow: 0rpx -4rpx 20rpx 0rpx rgba(0, 0, 0, 0.04);
}

.clean-details-btn-item{
    width: 630rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-radius: 56rpx;
}

.left-theme{
    background-color: #ffffff;
    box-shadow: 0rpx 5rpx 20rpx 0rpx rgba(0, 0, 0, 0.1);
    color: rgba(0, 0, 0, 0.85);
    font-size: 32rpx;
}

.right-theme{
    color: #ffffff;
