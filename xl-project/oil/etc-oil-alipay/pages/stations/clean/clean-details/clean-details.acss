.clean-no-support-title-text {
    color: #333333;
    font-size: 42rpx;
    text-align: center;
    font-weight: bolder;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: -50rpx;
}

.clean-no-support-title {
    margin: 30rpx;
    width: 200rpx;
    height: 30rpx;
}

.clean-no-support-text {
    /*width: 90%;*/
    text-align: left;
    color: #7e7e7e;
    font-size: 30rpx;
